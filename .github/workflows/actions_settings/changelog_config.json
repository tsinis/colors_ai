{
    "categories": [
        {
            "title": "## ğŸš€ Features",
            "labels": [
                "feature"
            ]
        },
        {
            "title": "## ğŸ› Fixes",
            "labels": [
                "fix"
            ]
        },
        {
            "title": "## ğŸ§ª Tests",
            "labels": [
                "test"
            ]
        },
        {
            "title": "## ğŸ§ª Tests and some ğŸª„ Magic",
            "labels": [
                "test",
                "magic"
            ],
            "exclude_labels": [
                "no-magic"
            ],
            "exhaustive": true,
            "empty_content": "- no matching PRs"
        }
    ],
    "ignore_labels": [
        "ignore",
        "dependencies",
        "ci/cd"
    ],
    "template": "${{UNCATEGORIZED}}",
    "pr_template": "- ${{TITLE}}\n   - PR: #${{NUMBER}}",
    "empty_template": "- no changes",
    "label_extractor": [
        {
            "pattern": "(.) (.+)",
            "target": "$1",
            "flags": "gu"
        },
        {
            "pattern": "\\[Issue\\]",
            "on_property": "title",
            "method": "match"
        }
    ],
    "duplicate_filter": {
        "pattern": "\\[ABC-....\\]",
        "on_property": "title",
        "method": "match"
    },
    "transformers": [
        {
            "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
            "target": "- $4\n  - $6"
        }
    ],
    "max_tags_to_fetch": 10,
    "max_pull_requests": 12,
    "max_back_track_time_days": 60,
    "tag_resolver": {
        "method": "semver",
        "filter": {
            "pattern": "api-(.+)",
            "flags": "gu"
        }
    },
    "base_branches": [
        "main"
    ]
}